{
  "name": "tagwithme",
  "version": "1.0.0",
  "description": "An app for getting people together at events.",
  "main": "app.js",
  "scripts": {
    "start-dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "test": "cross-env NODE_ENV=test mocha --timeout 5000 src/__tests__/*.test.js",
    "report": "cross-env NODE_ENV=test nyc --reporter=html mocha --timeout 5000 src/__tests__/*.test.js",
    "text-report": "cross-env NODE_ENV=test nyc --reporter=text-summary mocha --timeout 5000 src/__tests__/*.test.js",
    "nyc": "cross-env NODE_ENV=test nyc mocha --timeout 5000 src/__tests__/*.test.js",
    "start": "nodemon src/app.js"
  },
  "keywords": [
    "Social",
    "Events",
    "Concerts",
    "Meet"
  ],
  "author": "Binish Koirala",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.2"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cross-env": "^6.0.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.1"
  }
}
